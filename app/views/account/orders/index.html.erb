<div class="row">
  <div class="container">

    <h2 class="text-center title-cl-1">订单列表</h2>
    <br>
    <table class="table table-bordered table-striped table-hover">
      <thead>
        <tr>
          <th>#</th>
          <th>生成时间</th>
        </tr>
      </thead>
      <tbody>

        <% @orders.each do |order| %>
        <tr>
          <td><%= link_to(order.id, order_path(order.token)) %></td>
          <td><%= order.created_at.to_s(:long) %></td>
        </tr>
        <% end %>

      </tbody>
    </table>
  </div>
</div>
