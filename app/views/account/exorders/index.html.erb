<div class="container">
  <div class="row">
<h2 class="title-cl-1 text-center">订单列表 </h2>
<br>
<table class="table table-bordered">
  <thead>
    <tr>
      <th>#</th>
      <th>生成时间</th>

    </tr>
  </thead>
  <tbody>

    <% @exorders.each do |exorder| %>
    <tr>
      <td> <%= link_to(exorder.id, exorder_path(exorder.token)) %> </td>
      <td> <%= exorder.created_at.to_s(:long) %> </td>
    </tr>
    <% end %>

  </tbody>
</table>
  </div>
</div>
